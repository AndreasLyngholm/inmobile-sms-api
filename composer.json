{
    "name": "andreaslyngholm/inmobile-sms-api",
    "description": "PHP wrapper for interaction with the Inmobile SMS API",
    "keywords": ["inmobile", "sms", "textmessage", "php"],
    "type": "library",
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Inmobile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inmobile\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Martin van de Belt",
            "email": "martin@vandebelt.dk"
        },
        {
            "name": "Andreas Lyngholm Poulsen",
            "email": "andreas@simpleconcept.dk"
        }
    ]
}
